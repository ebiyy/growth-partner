{
	"$schema": "https://turbo.build/schema.json",
	"globalDependencies": [".env"],
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": [".next/**", "dist/**"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"test": {
			"dependsOn": ["^build"],
			"inputs": ["src/**/*.tsx", "src/**/*.ts", "test/**/*.ts", "test/**/*.tsx"]
		},
		"lint": {
			"outputs": []
		},
		"clean": {
			"cache": false
		}
	}
}
